<odoo>
    <!--Usuarios Internos-->
    <record id="access_jasper_server_user" model="ir.model.access">
        <field name="name">access.jasper.server.user</field>
        <field name="model_id" ref="jasper_integration.model_jasper_server"/>
        <field name="group_id" ref="base.group_user"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_create" eval="1"/>
        <field name="perm_unlink" eval="1"/>
        <field name="perm_write" eval="1"/>
    </record>

    <record id="access_jasper_server_report_user" model="ir.model.access">
        <field name="name">access.jasper.server.report.user</field>
        <field name="model_id" ref="jasper_integration.model_jasper_server_report"/>
        <field name="group_id" ref="base.group_user"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_create" eval="1"/>
        <field name="perm_unlink" eval="1"/>
        <field name="perm_write" eval="1"/>
    </record>
</odoo>